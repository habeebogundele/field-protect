# IMPORTANT: Copy this file to .env and update with your actual values

# 1. MONGODB_URI - Your MongoDB Atlas connection string
# Get from: MongoDB Atlas -> Connect -> Connect your application
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/fieldshare

# 2. SESSION_SECRET - Random secret for JWT sessions
# Generate with: openssl rand -base64 32
SESSION_SECRET=

# 3. ADMIN_SIGNUP_CODE - Secret code for creating admin accounts
# IMPORTANT: Change from default! This protects your admin portal.
ADMIN_SIGNUP_CODE=CHANGE_ME_IN_PRODUCTION

# 4. OWNER_EMAIL - Your admin contact email
OWNER_EMAIL=admin@example.com
